 #AdditionalRes: ..\res
 &lt;p&gt;
 Sub Globals
 Private HCycleViewPager As Hitex_HorizontalCycleViewPager
 Private Panel(8) As Panel
 End Sub
 ________________________________________________________________
 &lt;p&gt;
 Sub Activity_Create(FirstTime As Boolean)
 &lt;p&gt;
 Activity.Color = 0xFFFAFAFA
 HCycleViewPager.Initialize("TopViewPager")
 &lt;p&gt;
 For i = 0 To Panel.Length -1
 Panel(i).Initialize("Panel" &amp;amp; i)
 Panel(i).Color = Colors.RGB(Rnd(0,255),Rnd(0,255),Rnd(0,255))
 Next
 &lt;p&gt;
 Activity.AddView(HCycleViewPager,0,0,100%x,100%y)
 &lt;p&gt;
 HCycleViewPager.SetAdapter(Panel,True,False)
 &lt;p&gt;
 End Sub
 ________________________________________________________________</comment>
