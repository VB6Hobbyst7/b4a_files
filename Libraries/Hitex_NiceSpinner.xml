	Public NiceSpinner As Hitex_NiceSpinner
	Activity.Color = 0xFFF7F7F7
	NiceSpinner.Initialize("NiceSpinner",False,False)
	Activity.AddView(NiceSpinner,5%x,10%y,90%x,10%y)
	NiceSpinner.TextColor = Colors.Black
	NiceSpinner.Gravity = Gravity.CENTER_HORIZONTAL
	NiceSpinner.Typeface = Typeface.LoadFromAssets("iransans_light.ttf")
	NiceSpinner.AddItems(Array As String("Select a food : ","Noddle","Soup","Steak","Pasta","Pizza"))
	
	
	Event : 

	Sub NiceSpinner_ItemClick (Position As Int, Value As Object)
	ToastMessageShow(Position &amp;amp; "  _  " &amp;amp; Value,False)
	End Sub
	
